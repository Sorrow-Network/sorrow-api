{
  "name": "sorrowapi",
  "version": "1.0.1",
  "description": "All-in-one API for random animal, anime, and more images, roasts and jokes.",
  "main": "dist/index.ts",
  "scripts": {
    "build": "npx tsup ./src/index.ts --minify",
    "test": "nyc ava",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sorrow-Network/sorrow-api.git"
  },
  "publishConfig": {
  "registry":"https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "random-meow",
    "dog",
    "cat",
    "bird",
    "panda",
    "koala",
    "animal api",
    "random animal image api"
  ],
  "author": "treboryx <treboryx@pm.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sorrow-Network/sorrow-api/issues"
  },
  "homepage": "https://github.com/Sorrow-Network/sorrow-api#readme",
  "dependencies": {
    "node-superfetch": "^0.1.11"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "devDependencies": {
    "ava": "^3.14.0",
    "import-to-array": "^1.0.0",
    "nyc": "^15.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
